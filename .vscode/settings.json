{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.enable": true,
  "eslint.validate": ["javascript"],
  "files.associations": {
    "*.js": "javascript",
    "*.css": "css",
    "*.html": "html"
  },
  "path-intellisense.mappings": {
    "@": "${workspaceRoot}"
  },
  "spellright.language": ["en"],
  "spellright.documentTypes": ["javascript", "html", "plaintext"]
}